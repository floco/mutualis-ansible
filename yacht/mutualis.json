[
  {
    "type": 1,
    "title": "Pyload",
    "name": "pyload",
    "description": "Web Interface to download",
    "categories": [
      "Download"
    ],
    "platform": "linux",
    "logo": "https://developer.asustor.com/uploadIcons/0020_999_1577779483_pyload_256.png",
    "image": "writl/pyload:latest",
    "restart_policy": "unless-stopped",
    "env": [
      {
        "name": "PUID",
        "label": "PUID",
        "default": "!PUID"
      },
      {
        "name": "PGID",
        "label": "PGID",
        "default": "!PGID"
      }
    ],
    "ports": [
      {
        "WebUI": "8001:8000/tcp"
      }
    ],
    "volumes": [
      {
        "container": "/opt/pyload/Downloads",
        "bind": "!downloads"
      },
      {
        "container": "/opt/pyload/pyload-config",
        "bind": "!config/pyload"
      }
    ]
  },
  {
    "type": 1,
    "title": "Fava",
    "name": "fava",
    "description": "Web Interface for beancount accounting",
    "categories": [
      "Finance"
    ],
    "platform": "linux",
    "logo": "https://beancount.github.io/fava/_static/logo.png",
    "image": "grostim/fava-docker",
    "restart_policy": "unless-stopped",
    "env": [
      {
        "name": "PUID",
        "label": "PUID",
        "default": "!PUID"
      },
      {
        "name": "PGID",
        "label": "PGID",
        "default": "!PGID"
      },
      {
        "name": "BEANCOUNT_FILE",
        "label": "BEANCOUNT_FILE",
        "default": "/root/main.beancount"
      }
    ],
    "ports": [
      {
        "WebUI": "8002:5000/tcp"
      }
    ],
    "volumes": [
      {
        "container": "/root",
        "bind": "!finance"
      }
    ]
  },
  {
    "type": 1,
    "title": "Adguardhome",
    "name": "adguardhome",
    "description": "Ad blocker like PIHole",
    "categories": [
      "DNS"
    ],
    "platform": "linux",
    "logo": "https://developer.asustor.com/uploadIcons/0020_999_1595573041_AdGuardhome_256.png",
    "image": "adguard/adguardhome",
    "restart_policy": "unless-stopped",
    "env": [
      {
        "name": "PUID",
        "label": "PUID",
        "default": "!PUID"
      },
      {
        "name": "PGID",
        "label": "PGID",
        "default": "!PGID"
      }
    ],
    "ports": [
      {
        "dns-tcp": "53:53/tcp"
      },
      {
        "dns-udp": "53:53/udp"
      },
      {
        "dhcp-udp": "67:67/udp"
      },
      {
        "dhcp-tcp": "68:68/tcp"
      },
      {
        "thing": "853:853/tcp"
      },
      {
        "WebUI": "8003:45158/tcp"
      }
    ],
    "volumes": [
      {
        "container": "/opt/adguardhome/work",
        "bind": "!config/adguard/work"
      },
      {
        "container": "/opt/adguardhome/conf",
        "bind": "!config/adguard/conf"
      }
    ]
  },
  {
    "type": 1,
    "title": "Portainer",
    "name": "portainer",
    "description": "Docker Management",
    "categories": [
      "Management"
    ],
    "platform": "linux",
    "logo": "https://developer.asustor.com/uploadIcons/0020_999_1596450839_Portainer_256.png",
    "image": "adguard/adguardhome",
    "restart_policy": "unless-stopped",
    "env": [
      {
        "name": "PUID",
        "label": "PUID",
        "default": "!PUID"
      },
      {
        "name": "PGID",
        "label": "PGID",
        "default": "!PGID"
      }
    ],
    "ports": [
      {
        "WebUI": "9000:9000/tcp"
      }
    ],
    "volumes": [
      {
        "container": "/var/run/docker.sock",
        "bind": "/var/run/docker.sock"
      },
      {
        "container": "/data",
        "bind": "!config/portainer"
      }
    ]
  }
]